{
    "env": {
        "browser": true,
        "es6": true,
        "node":true,
        "jest":true
    },
    "extends": ["eslint:recommended","plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "parser": "babel-eslint",
  	"plugins": [
    		"react"
  	],
    "rules": {
      "no-console": "off"
    }
}
